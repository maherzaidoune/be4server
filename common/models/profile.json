{
  "name": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phNumber": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "birthDate": {
      "type": "date",
      "required": true
    },
    "sex": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "picture": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "User": {
        "type": "belongsTo",
        "model": "User",
        "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
