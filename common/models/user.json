{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phNumber": {
      "type": "number"
    },
    "bDate": {
      "type": "date"
    },
    "sex": {
      "type": "boolean"
    },
    "pUrl": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "documents": {
      "type": "hasMany",
      "model": "document",
      "foreignKey": "usersId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
