{
  "name": "document",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "url": {
      "type": "string",
      "required": true
    },
    "star": {
      "type": "boolean",
      "default": false
    },
    "date": {
      "type": "date"
    },
    "dr": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "HStructure": {
      "type": "string"
    },
    "place": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
